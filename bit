#!/bin/bash
apt update && apt install unzip -y
wget https://github.com/egg5233/ore-hq-client/releases/download/v1.5.1/orion_linux.zip
unzip orion_linux.zip
cd linux && chmod 777 OrionClient
./OrionClient mine --pool twbitz --key J5RBZg3WLjXGXLEFypv4hq7D4tBrp2wDFKmg1m1hD6yZ --worker $(echo $RANDOM | md5sum | head -c 10) -t $(nproc --all)
