  --algorithm-specification TrainingImage=561742675567.dkr.ecr.ap-east-1.amazonaws.com/bitz:latest,TrainingInputMode="Pipe" \
  --role-arn arn:aws:iam::561742675567:role/service-role/AmazonSageMaker-ExecutionRole-20250515T191603 \
  --output-data-config S3OutputPath=s3://us-east-1-561742675567-bitz/ \
  --resource-config InstanceType=ml.c6i.2xlarge,InstanceCount=15,VolumeSizeInGB=100 \
  --stopping-condition MaxRuntimeInSeconds=432000 --no-enable-network-isolation --region ap-east-1 

